{
  "appenders": {
    "fileOutput": {
      "type": "file",
      "alwaysIncludePattern": true,
      "maxLogSize": 10485760,
      "filename": "",
      "layout": {
        "type": "pattern",
        "pattern": "[%d{yyyy-MM-dd hh:mm:ss}] [%p] %m"
      }
    },
    "consoleOutput": {
      "type": "stdout"
    }
  },
  "categories": {
    "default": {
      "appenders": ["fileOutput", "consoleOutput"],
      "level": "debug",
      "enableCallStack": true
    }
  }
}