{
  "name": "mqttx-cli",
  "version": "1.8.1",
  "description": "MQTT X Command Line Tools",
  "keywords": [
    "mqtt",
    "publish/subscribe",
    "publish",
    "subscribe"
  ],
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc"
  },
  "main": "index.js",
  "bin": {
    "mqttx": "./lib/src/index.js"
  },
  "license": "Apache",
  "dependencies": {
    "commander": "^9.3.0",
    "concat-stream": "^2.0.0",
    "mqtt": "^4.3.7",
    "pump": "^3.0.0",
    "readable-stream": "^3.6.0",
    "split2": "^4.1.0"
  },
  "devDependencies": {
    "@types/concat-stream": "^2.0.0",
    "@types/node": "^17.0.43",
    "@types/pump": "^1.1.1",
    "@types/readable-stream": "^2.3.13",
    "@types/split2": "^3.2.1",
    "@types/ws": "^8.5.3",
    "typescript": "^4.7.3"
  },
  "pkg": {
    "targets": [
      "node14-linux-x64",
      "node14-macos-x64",
      "node14-win-x64",
      "node14-alpine-x64",
      "node14-linux-arm64",
      "node14-macos-arm64",
      "node14-win-arm64",
      "node14-alpine-arm64"
    ],
    "outputPath": "release"
  }
}
