{
  "name": "mqttx-cli",
  "version": "1.9.2",
  "description": "MQTTX Command Line Tools",
  "keywords": [
    "mqtt",
    "publish/subscribe",
    "publish",
    "subscribe"
  ],
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc"
  },
  "main": "./dist/src/index.js",
  "bin": {
    "mqttx": "./bin/index.js"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "~4.1.2",
    "commander": "^9.3.0",
    "compare-versions": "^5.0.1",
    "concat-stream": "^2.0.0",
    "core-js": "^3.26.0",
    "mqtt": "^4.3.7",
    "pump": "^3.0.0",
    "readable-stream": "^3.6.0",
    "signale": "^1.4.0",
    "split2": "^4.1.0",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@types/concat-stream": "^2.0.0",
    "@types/node": "^17.0.43",
    "@types/pump": "^1.1.1",
    "@types/readable-stream": "^2.3.13",
    "@types/signale": "^1.4.4",
    "@types/split2": "^3.2.1",
    "@types/ws": "^8.5.3",
    "typescript": "^4.7.3"
  },
  "pkg": {
    "targets": [
      "node16-linux-x64",
      "node16-macos-x64",
      "node16-win-x64",
      "node16-alpine-x64",
      "node16-linux-arm64",
      "node16-macos-arm64",
      "node16-win-arm64",
      "node16-alpine-arm64"
    ],
    "outputPath": "release"
